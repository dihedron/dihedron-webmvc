<domains xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="domains.xsd">
	<!--
		THIS IS THE DEFAULT DOMAIN, APPLICABLE TO ALL RESOURCES THAT HAVE NONE
		MORE SPECIFIC; IT USES THE DEFAULT STACK; WHICH MAY BE REPLACED BY THE
		EMPTY STACK WHEN DEBUGGING IS OVER AND PERFORMANCE BECOMES PARAMOUNT
	 -->
	<domain id="default">		
		<pattern>.*</pattern>
		<stack>default</stack>		
		<results>
			<result id="uncaught_exception" renderer="jsp">internal_error.jsp</result>
		</results>
	</domain>
</domains>