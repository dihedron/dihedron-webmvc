<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="stack">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="interceptors" />
				<xs:element ref="results" minOccurs="0" />
			</xs:sequence>
			<xs:attribute name="id" type="xs:normalizedString" use="required" />
		</xs:complexType>
	</xs:element>
	<xs:element name="result">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:normalizedString">
					<xs:attribute name="value" type="xs:normalizedString"
						use="required" />
					<xs:attribute name="renderer" type="xs:normalizedString"
						use="optional" default="jsp" />
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="results">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="result" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="parameter">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:normalizedString">
					<xs:attribute name="key" type="xs:normalizedString"
						use="required" />
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="interceptor">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="parameter" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
			<xs:attribute name="id" type="xs:normalizedString" use="required" />
			<xs:attribute name="class" type="xs:normalizedString"
				use="required" />
		</xs:complexType>
	</xs:element>
	<xs:element name="interceptors">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="interceptor" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="configuration">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="stack" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>